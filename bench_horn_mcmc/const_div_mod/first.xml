<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>2</ARITY>
      <NAME>inv</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>i</VAR>
    <VAR>i1</VAR>
    <VAR>j</VAR>
    <VAR>j1</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>i</VAR>
      <VAR>j</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>i</VAR>
          <VAR>j</VAR>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>i</VAR>
      <VAR>j</VAR>
    </BEFORE>
    <AFTER>
      <VAR>i1</VAR>
      <VAR>j</VAR>
    </AFTER>
    <OR>
      <NOT>
        <GT>
          <VAR>i</VAR>
          <INT>0</INT>
        </GT>
      </NOT>
      <NOT>
        <EQ>
          <MOD>
            <VAR>i</VAR>
            <INT>2</INT>
          </MOD>
          <INT>0</INT>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>i1</VAR>
          <DIV>
            <VAR>i</VAR>
            <INT>2</INT>
          </DIV>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>i</VAR>
      <VAR>j</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>i</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
      <EQ>
        <MOD>
          <VAR>j</VAR>
          <INT>2</INT>
        </MOD>
        <INT>0</INT>
      </EQ>
    </OR>
  </ASSERT>
</VC>

