<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>3</ARITY>
      <NAME>inv</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>x0</VAR>
    <VAR>x1</VAR>
    <VAR>y0</VAR>
    <VAR>y1</VAR>
    <VAR>flag</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>x1</VAR>
      <VAR>y1</VAR>
      <VAR>flag</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>x1</VAR>
          <INT>2</INT>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>y1</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>x0</VAR>
      <VAR>y0</VAR>
      <VAR>flag</VAR>
    </BEFORE>
    <AFTER>
      <VAR>x1</VAR>
      <VAR>y1</VAR>
      <VAR>flag</VAR>
    </AFTER>
    <OR>
      <AND>
        <OR>
          <NOT>
            <EQ>
              <VAR>flag</VAR>
              <INT>0</INT>
            </EQ>
          </NOT>
          <NOT>
            <EQ>
              <VAR>x1</VAR>
              <PLUS>
                <VAR>x0</VAR>
                <INT>4</INT>
              </PLUS>
            </EQ>
          </NOT>
          <NOT>
            <EQ>
              <VAR>y1</VAR>
              <VAR>y0</VAR>
            </EQ>
          </NOT>
        </OR>
        <OR>
          <EQ>
            <VAR>flag</VAR>
            <INT>0</INT>
          </EQ>
          <NOT>
            <EQ>
              <VAR>x1</VAR>
              <PLUS>
                <VAR>x0</VAR>
                <INT>2</INT>
              </PLUS>
            </EQ>
          </NOT>
          <NOT>
            <EQ>
              <VAR>y1</VAR>
              <PLUS>
                <VAR>y0</VAR>
                <INT>1</INT>
              </PLUS>
            </EQ>
          </NOT>
        </OR>
      </AND>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>x1</VAR>
      <VAR>y1</VAR>
      <VAR>flag</VAR>
    </VARDECLS>
    <OR>
      <EQ>
        <VAR>y1</VAR>
        <INT>0</INT>
      </EQ>
      <EQ>
        <VAR>x1</VAR>
        <PLUS>
          <INT>2</INT>
          <MUL>
            <INT>2</INT>
            <VAR>y1</VAR>
          </MUL>
        </PLUS>
      </EQ>
    </OR>
  </ASSERT>
</VC>

