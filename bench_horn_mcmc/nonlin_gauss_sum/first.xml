<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>3</ARITY>
      <NAME>inv</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>i</VAR>
    <VAR>n</VAR>
    <VAR>sum</VAR>
    <VAR>i1</VAR>
    <VAR>sum1</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>i</VAR>
      <VAR>n</VAR>
      <VAR>sum</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>sum</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>i</VAR>
          <INT>1</INT>
        </EQ>
      </NOT>
      <NOT>
        <LE>
          <INT>1</INT>
          <VAR>n</VAR>
        </LE>
      </NOT>
      <NOT>
        <LE>
          <VAR>n</VAR>
          <INT>1000</INT>
        </LE>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>i</VAR>
      <VAR>n</VAR>
      <VAR>sum</VAR>
    </BEFORE>
    <AFTER>
      <VAR>i1</VAR>
      <VAR>n</VAR>
      <VAR>sum1</VAR>
    </AFTER>
    <OR>
      <NOT>
        <LT>
          <VAR>i</VAR>
          <VAR>n</VAR>
        </LT>
      </NOT>
      <NOT>
        <EQ>
          <VAR>sum1</VAR>
          <PLUS>
            <VAR>sum</VAR>
            <VAR>i</VAR>
          </PLUS>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>i1</VAR>
          <PLUS>
            <VAR>i</VAR>
            <INT>1</INT>
          </PLUS>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>i</VAR>
      <VAR>n</VAR>
      <VAR>sum</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>i</VAR>
          <VAR>n</VAR>
        </EQ>
      </NOT>
      <EQ>
        <PLUS>
          <MUL>
            <INT>2</INT>
            <VAR>sum</VAR>
          </MUL>
          <MUL>
            <INT>2</INT>
            <VAR>n</VAR>
          </MUL>
        </PLUS>
        <MUL>
          <VAR>n</VAR>
          <PLUS>
            <VAR>n</VAR>
            <INT>1</INT>
          </PLUS>
        </MUL>
      </EQ>
    </OR>
  </ASSERT>
</VC>

