<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>3</ARITY>
      <NAME>inv</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>a</VAR>
    <VAR>b</VAR>
    <VAR>b1</VAR>
    <VAR>i</VAR>
    <VAR>i1</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>a</VAR>
      <VAR>b</VAR>
      <VAR>i</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <LT>
          <VAR>a</VAR>
          <INT>0</INT>
        </LT>
      </NOT>
      <NOT>
        <EQ>
          <VAR>b</VAR>
          <VAR>a</VAR>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>i</VAR>
          <INT>-1</INT>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>a</VAR>
      <VAR>b</VAR>
      <VAR>i</VAR>
    </BEFORE>
    <AFTER>
      <VAR>a</VAR>
      <VAR>b1</VAR>
      <VAR>i1</VAR>
    </AFTER>
    <OR>
      <NOT>
        <EQ>
          <VAR>b1</VAR>
          <MUL>
            <VAR>a</VAR>
            <VAR>b</VAR>
          </MUL>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>i1</VAR>
          <MINUS>
            <VAR>i</VAR>
          </MINUS>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>a</VAR>
      <VAR>b</VAR>
      <VAR>i</VAR>
    </VARDECLS>
    <OR>
      <GT>
        <MUL>
          <VAR>b</VAR>
          <VAR>i</VAR>
        </MUL>
        <INT>0</INT>
      </GT>
    </OR>
  </ASSERT>
</VC>

