      </NOT>
      <NOT>
        <LE>
          <VAR>k</VAR>
          <INT>10</INT>
        </LE>
      </NOT>
      <NOT>
        <GE>
          <VAR>LRG</VAR>
          <INT>0</INT>
        </GE>
      </NOT>
      <NOT>
        <EQ>
          <VAR>i</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>i_</VAR>
      <VAR>k</VAR>
      <VAR>LRG</VAR>
    </BEFORE>
    <AFTER>
      <VAR>i</VAR>
      <VAR>k</VAR>
      <VAR>LRG</VAR>
