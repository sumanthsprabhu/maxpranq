<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>3</ARITY>
      <NAME>itp</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>i</VAR>
    <VAR>i_</VAR>
    <VAR>k</VAR>
    <VAR>LRG</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>i</VAR>
      <VAR>k</VAR>
      <VAR>LRG</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <LE>
          <INT>0</INT>
          <VAR>k</VAR>
