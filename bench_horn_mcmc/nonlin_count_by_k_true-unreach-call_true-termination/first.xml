    <OR>
      <NOT>
        <LT>
          <VAR>i_</VAR>
          <MUL>
            <VAR>LRG</VAR>
            <VAR>k</VAR>
          </MUL>
        </LT>
      </NOT>
      <NOT>
        <EQ>
          <VAR>i</VAR>
          <PLUS>
            <VAR>i_</VAR>
            <VAR>k</VAR>
          </PLUS>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>i</VAR>
      <VAR>k</VAR>
      <VAR>LRG</VAR>
    </VARDECLS>
    <OR>
      <LT>
        <VAR>i</VAR>
        <MUL>
          <VAR>LRG</VAR>
          <VAR>k</VAR>
        </MUL>
      </LT>
      <EQ>
        <VAR>i</VAR>
        <MUL>
          <VAR>LRG</VAR>
          <VAR>k</VAR>
        </MUL>
      </EQ>
    </OR>
  </ASSERT>
