<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>4</ARITY>
      <NAME>inv</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>x</VAR>
    <VAR>x1</VAR>
    <VAR>a</VAR>
    <VAR>a1</VAR>
    <VAR>b</VAR>
    <VAR>b1</VAR>
    <VAR>len</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>x</VAR>
      <VAR>a</VAR>
      <VAR>b</VAR>
      <VAR>len</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>x</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>a</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>b</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
      <NOT>
        <GE>
          <VAR>len</VAR>
          <INT>0</INT>
        </GE>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>x</VAR>
      <VAR>a</VAR>
      <VAR>b</VAR>
      <VAR>len</VAR>
    </BEFORE>
    <AFTER>
      <VAR>x1</VAR>
      <VAR>a1</VAR>
      <VAR>b1</VAR>
      <VAR>len</VAR>
    </AFTER>
    <OR>
      <NOT>
        <LT>
          <VAR>x</VAR>
          <VAR>len</VAR>
        </LT>
      </NOT>
      <NOT>
        <EQ>
          <VAR>x1</VAR>
          <PLUS>
            <VAR>x</VAR>
            <INT>1</INT>
          </PLUS>
        </EQ>
      </NOT>
      <AND>
        <OR>
          <NOT>
            <EQ>
              <VAR>a1</VAR>
              <PLUS>
                <VAR>a</VAR>
                <INT>1</INT>
              </PLUS>
            </EQ>
          </NOT>
          <NOT>
            <EQ>
              <VAR>b1</VAR>
              <PLUS>
                <VAR>b</VAR>
                <INT>2</INT>
              </PLUS>
            </EQ>
          </NOT>
        </OR>
        <OR>
          <NOT>
            <EQ>
              <VAR>a1</VAR>
              <PLUS>
                <VAR>a</VAR>
                <INT>2</INT>
              </PLUS>
            </EQ>
          </NOT>
          <NOT>
            <EQ>
              <VAR>b1</VAR>
              <PLUS>
                <VAR>b</VAR>
                <INT>1</INT>
              </PLUS>
            </EQ>
          </NOT>
        </OR>
      </AND>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>x</VAR>
      <VAR>a</VAR>
      <VAR>b</VAR>
      <VAR>len</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <GE>
          <VAR>x</VAR>
          <VAR>len</VAR>
        </GE>
      </NOT>
      <EQ>
        <PLUS>
          <VAR>a</VAR>
          <VAR>b</VAR>
        </PLUS>
        <MUL>
          <INT>3</INT>
          <VAR>len</VAR>
        </MUL>
      </EQ>
    </OR>
  </ASSERT>
</VC>

