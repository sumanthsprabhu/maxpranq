<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>4</ARITY>
      <NAME>itp</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>i1</VAR>
    <VAR>i1p</VAR>
    <VAR>i2</VAR>
    <VAR>i2p</VAR>
    <VAR>N1</VAR>
    <VAR>N2</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>i1</VAR>
      <VAR>i2</VAR>
      <VAR>N1</VAR>
      <VAR>N2</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>N1</VAR>
          <VAR>N2</VAR>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>i1</VAR>
          <VAR>N1</VAR>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>i2</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>i1</VAR>
      <VAR>i2</VAR>
      <VAR>N1</VAR>
      <VAR>N2</VAR>
    </BEFORE>
    <AFTER>
      <VAR>i1p</VAR>
      <VAR>i2p</VAR>
      <VAR>N1</VAR>
      <VAR>N2</VAR>
    </AFTER>
    <OR>
      <NOT>
        <GT>
          <VAR>i1</VAR>
          <INT>0</INT>
        </GT>
      </NOT>
      <NOT>
        <LT>
          <VAR>i2</VAR>
          <VAR>N1</VAR>
        </LT>
      </NOT>
      <NOT>
        <EQ>
          <VAR>i1p</VAR>
          <MINUS>
            <VAR>i1</VAR>
            <INT>1</INT>
          </MINUS>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>i2p</VAR>
          <PLUS>
            <VAR>i2</VAR>
            <INT>1</INT>
          </PLUS>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>i1</VAR>
      <VAR>i2</VAR>
      <VAR>N1</VAR>
      <VAR>N2</VAR>
    </VARDECLS>
    <OR>
      <EQ>
        <EQ>
          <VAR>i2</VAR>
          <VAR>N1</VAR>
        </EQ>
        <EQ>
          <VAR>i1</VAR>
          <INT>0</INT>
        </EQ>
      </EQ>
    </OR>
  </ASSERT>
</VC>

