<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>2</ARITY>
      <NAME>itp</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>x0</VAR>
    <VAR>x1</VAR>
    <VAR>x2</VAR>
    <VAR>x3</VAR>
    <VAR>x4</VAR>
    <VAR>x5</VAR>
    <VAR>x6</VAR>
    <VAR>x7</VAR>
    <VAR>x8</VAR>
    <VAR>x9</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>x1</VAR>
      <VAR>x3</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>x1</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>x3</VAR>
          <INT>100</INT>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>x1</VAR>
      <VAR>x3</VAR>
    </BEFORE>
    <AFTER>
      <VAR>x2</VAR>
      <VAR>x4</VAR>
    </AFTER>
    <OR>
      <NOT>
        <EQ>
          <VAR>x2</VAR>
          <PLUS>
            <VAR>x1</VAR>
            <INT>1</INT>
          </PLUS>
        </EQ>
      </NOT>
      <AND>
        <OR>
          <NOT>
            <GT>
              <VAR>x3</VAR>
              <INT>10</INT>
            </GT>
          </NOT>
          <NOT>
            <EQ>
              <VAR>x4</VAR>
              <INT>0</INT>
            </EQ>
          </NOT>
        </OR>
        <OR>
          <NOT>
            <LE>
              <VAR>x3</VAR>
              <INT>10</INT>
            </LE>
          </NOT>
          <NOT>
            <EQ>
              <VAR>x4</VAR>
              <PLUS>
                <VAR>x3</VAR>
                <INT>1</INT>
              </PLUS>
            </EQ>
          </NOT>
        </OR>
      </AND>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>x1</VAR>
      <VAR>x3</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <GT>
          <VAR>x1</VAR>
          <INT>10</INT>
        </GT>
      </NOT>
      <LT>
        <VAR>x3</VAR>
        <VAR>x1</VAR>
      </LT>
    </OR>
  </ASSERT>
</VC>

