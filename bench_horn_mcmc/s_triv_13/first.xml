<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>2</ARITY>
      <NAME>inv</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>x</VAR>
    <VAR>x1</VAR>
    <VAR>y</VAR>
    <VAR>y1</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>x</VAR>
      <VAR>y</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <VAR>x</VAR>
      </NOT>
      <NOT>
        <EQ>
          <VAR>x</VAR>
          <EQ>
            <VAR>y</VAR>
            <INT>0</INT>
          </EQ>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>x</VAR>
      <VAR>y</VAR>
    </BEFORE>
    <AFTER>
      <VAR>x1</VAR>
      <VAR>y1</VAR>
    </AFTER>
    <OR>
      <NOT>
        <EQ>
          <VAR>y1</VAR>
          <PLUS>
            <VAR>y</VAR>
            <INT>1</INT>
          </PLUS>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>x</VAR>
      <VAR>y</VAR>
    </VARDECLS>
    <OR>
      <GE>
        <VAR>y</VAR>
        <INT>0</INT>
      </GE>
    </OR>
  </ASSERT>
</VC>

