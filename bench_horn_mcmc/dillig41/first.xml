<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>3</ARITY>
      <NAME>inv</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>x0</VAR>
    <VAR>x1</VAR>
    <VAR>y0</VAR>
    <VAR>y1</VAR>
    <VAR>n</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>x1</VAR>
      <VAR>y1</VAR>
      <VAR>n</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>x1</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>y1</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
      <NOT>
        <GE>
          <VAR>n</VAR>
          <INT>0</INT>
        </GE>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>x0</VAR>
      <VAR>y0</VAR>
      <VAR>n</VAR>
    </BEFORE>
    <AFTER>
      <VAR>x1</VAR>
      <VAR>y1</VAR>
      <VAR>n</VAR>
    </AFTER>
    <OR>
      <NOT>
        <LT>
          <VAR>x0</VAR>
          <VAR>n</VAR>
        </LT>
      </NOT>
      <NOT>
        <EQ>
          <VAR>x1</VAR>
          <PLUS>
            <VAR>x0</VAR>
            <INT>1</INT>
          </PLUS>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>y1</VAR>
          <PLUS>
            <VAR>y0</VAR>
            <VAR>x1</VAR>
          </PLUS>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>x1</VAR>
      <VAR>y1</VAR>
      <VAR>n</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <GE>
          <VAR>x1</VAR>
          <VAR>n</VAR>
        </GE>
      </NOT>
      <GT>
        <PLUS>
          <INT>1</INT>
          <VAR>x1</VAR>
          <VAR>y1</VAR>
        </PLUS>
        <MUL>
          <INT>2</INT>
          <VAR>n</VAR>
        </MUL>
      </GT>
    </OR>
  </ASSERT>
</VC>

