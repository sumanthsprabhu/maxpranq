<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>3</ARITY>
      <NAME>itp1</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>x1</VAR>
    <VAR>y1</VAR>
    <VAR>z1</VAR>
    <VAR>x2</VAR>
    <VAR>y2</VAR>
    <VAR>z2</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>x1</VAR>
      <VAR>y1</VAR>
      <VAR>z1</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>x1</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>y1</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>z1</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>x1</VAR>
      <VAR>y1</VAR>
      <VAR>z1</VAR>
    </BEFORE>
    <AFTER>
      <VAR>x2</VAR>
      <VAR>y2</VAR>
      <VAR>z2</VAR>
    </AFTER>
    <OR>
      <AND>
        <OR>
          <NOT>
            <EQ>
              <VAR>x2</VAR>
              <PLUS>
                <VAR>x1</VAR>
                <INT>1</INT>
              </PLUS>
            </EQ>
          </NOT>
          <NOT>
            <EQ>
              <VAR>y2</VAR>
              <VAR>y1</VAR>
            </EQ>
          </NOT>
          <NOT>
            <EQ>
              <VAR>z2</VAR>
              <PLUS>
                <VAR>z1</VAR>
                <INT>1</INT>
              </PLUS>
            </EQ>
          </NOT>
        </OR>
        <OR>
          <NOT>
            <EQ>
              <VAR>y2</VAR>
              <PLUS>
                <VAR>y1</VAR>
                <INT>1</INT>
              </PLUS>
            </EQ>
          </NOT>
          <NOT>
            <EQ>
              <VAR>x2</VAR>
              <VAR>x1</VAR>
            </EQ>
          </NOT>
          <NOT>
            <EQ>
              <VAR>z2</VAR>
              <MINUS>
                <VAR>z1</VAR>
                <INT>1</INT>
              </MINUS>
            </EQ>
          </NOT>
        </OR>
      </AND>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>x1</VAR>
      <VAR>y1</VAR>
      <VAR>z1</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>x1</VAR>
          <VAR>y1</VAR>
        </EQ>
      </NOT>
      <EQ>
        <VAR>z1</VAR>
        <INT>0</INT>
      </EQ>
    </OR>
  </ASSERT>
</VC>

