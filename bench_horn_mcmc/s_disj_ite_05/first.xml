<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>2</ARITY>
      <NAME>inv</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>x</VAR>
    <VAR>x2</VAR>
    <VAR>y</VAR>
    <VAR>y2</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>x</VAR>
      <VAR>y</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>x</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>y</VAR>
          <INT>50</INT>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>x</VAR>
      <VAR>y</VAR>
    </BEFORE>
    <AFTER>
      <VAR>x2</VAR>
      <VAR>y2</VAR>
    </AFTER>
    <OR>
      <NOT>
        <LT>
          <VAR>x</VAR>
          <INT>100</INT>
        </LT>
      </NOT>
      <NOT>
        <EQ>
          <VAR>x2</VAR>
          <PLUS>
            <VAR>x</VAR>
            <INT>1</INT>
          </PLUS>
        </EQ>
      </NOT>
      <AND>
        <OR>
          <NOT>
            <GT>
              <VAR>x2</VAR>
              <INT>50</INT>
            </GT>
          </NOT>
          <NOT>
            <EQ>
              <VAR>y2</VAR>
              <PLUS>
                <VAR>y</VAR>
                <INT>1</INT>
              </PLUS>
            </EQ>
          </NOT>
        </OR>
        <OR>
          <NOT>
            <LE>
              <VAR>x2</VAR>
              <INT>50</INT>
            </LE>
          </NOT>
          <NOT>
            <EQ>
              <VAR>y2</VAR>
              <VAR>y</VAR>
            </EQ>
          </NOT>
        </OR>
      </AND>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>x</VAR>
      <VAR>y</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>x</VAR>
          <INT>100</INT>
        </EQ>
      </NOT>
      <EQ>
        <VAR>y</VAR>
        <INT>100</INT>
      </EQ>
    </OR>
  </ASSERT>
</VC>

