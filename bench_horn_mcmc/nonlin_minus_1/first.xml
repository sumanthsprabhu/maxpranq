<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>3</ARITY>
      <NAME>inv</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>a</VAR>
    <VAR>a2</VAR>
    <VAR>n</VAR>
    <VAR>n2</VAR>
    <VAR>b</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>a</VAR>
      <VAR>b</VAR>
      <VAR>n</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <GT>
          <VAR>a</VAR>
          <INT>0</INT>
        </GT>
      </NOT>
      <NOT>
        <GT>
          <VAR>b</VAR>
          <INT>0</INT>
        </GT>
      </NOT>
      <NOT>
        <EQ>
          <VAR>n</VAR>
          <MUL>
            <VAR>a</VAR>
            <VAR>b</VAR>
          </MUL>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>a</VAR>
      <VAR>b</VAR>
      <VAR>n</VAR>
    </BEFORE>
    <AFTER>
      <VAR>a2</VAR>
      <VAR>b</VAR>
      <VAR>n2</VAR>
    </AFTER>
    <OR>
      <NOT>
        <EQ>
          <VAR>n2</VAR>
          <MINUS>
            <VAR>n</VAR>
            <VAR>b</VAR>
          </MINUS>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>a2</VAR>
          <MINUS>
            <VAR>a</VAR>
            <INT>1</INT>
          </MINUS>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>a</VAR>
      <VAR>b</VAR>
      <VAR>n</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>n</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
      <EQ>
        <VAR>a</VAR>
        <INT>0</INT>
      </EQ>
    </OR>
  </ASSERT>
</VC>

