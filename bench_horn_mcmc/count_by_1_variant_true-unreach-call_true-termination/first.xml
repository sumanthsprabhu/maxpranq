<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>2</ARITY>
      <NAME>itp</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>i</VAR>
    <VAR>i_</VAR>
    <VAR>LRG</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>i</VAR>
      <VAR>LRG</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>i</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
      <EQ>
        <VAR>i</VAR>
        <VAR>LRG</VAR>
      </EQ>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>i_</VAR>
      <VAR>LRG</VAR>
    </BEFORE>
    <AFTER>
      <VAR>i</VAR>
      <VAR>LRG</VAR>
    </AFTER>
    <OR>
      <NOT>
        <EQ>
          <VAR>i</VAR>
          <PLUS>
            <INT>1</INT>
            <VAR>i_</VAR>
          </PLUS>
        </EQ>
      </NOT>
      <EQ>
        <VAR>i</VAR>
        <VAR>LRG</VAR>
      </EQ>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>i</VAR>
      <VAR>LRG</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <GT>
          <VAR>i</VAR>
          <VAR>LRG</VAR>
        </GT>
      </NOT>
      <NOT>
        <GT>
          <VAR>LRG</VAR>
          <INT>10000</INT>
        </GT>
      </NOT>
    </OR>
  </ASSERT>
</VC>

