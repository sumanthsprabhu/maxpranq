<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>3</ARITY>
      <NAME>inv</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>x0</VAR>
    <VAR>x1</VAR>
    <VAR>y0</VAR>
    <VAR>y1</VAR>
    <VAR>n</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>x1</VAR>
      <VAR>y1</VAR>
      <VAR>n</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>n</VAR>
          <VAR>y1</VAR>
        </EQ>
      </NOT>
      <EQ>
        <VAR>x1</VAR>
        <VAR>y1</VAR>
      </EQ>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>x0</VAR>
      <VAR>y0</VAR>
      <VAR>n</VAR>
    </BEFORE>
    <AFTER>
      <VAR>x0</VAR>
      <VAR>y1</VAR>
      <VAR>n</VAR>
    </AFTER>
    <OR>
      <NOT>
        <GT>
          <VAR>x0</VAR>
          <INT>0</INT>
        </GT>
      </NOT>
      <NOT>
        <EQ>
          <VAR>y1</VAR>
          <PLUS>
            <VAR>y0</VAR>
            <VAR>x0</VAR>
          </PLUS>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>x1</VAR>
      <VAR>y1</VAR>
      <VAR>n</VAR>
    </VARDECLS>
    <OR>
      <GE>
        <VAR>y1</VAR>
        <VAR>n</VAR>
      </GE>
    </OR>
  </ASSERT>
</VC>

