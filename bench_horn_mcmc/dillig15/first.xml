<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>3</ARITY>
      <NAME>itp</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>j</VAR>
    <VAR>k</VAR>
    <VAR>n</VAR>
    <VAR>j1</VAR>
    <VAR>k1</VAR>
    <VAR>n1</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>j</VAR>
      <VAR>k</VAR>
      <VAR>n</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <GE>
          <VAR>n</VAR>
          <INT>1</INT>
        </GE>
      </NOT>
      <NOT>
        <GE>
          <VAR>k</VAR>
          <VAR>n</VAR>
        </GE>
      </NOT>
      <NOT>
        <EQ>
          <VAR>j</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>j</VAR>
      <VAR>k</VAR>
      <VAR>n</VAR>
    </BEFORE>
    <AFTER>
      <VAR>j1</VAR>
      <VAR>k1</VAR>
      <VAR>n</VAR>
    </AFTER>
    <OR>
      <NOT>
        <LE>
          <VAR>j</VAR>
          <MINUS>
            <VAR>n</VAR>
            <INT>1</INT>
          </MINUS>
        </LE>
      </NOT>
      <NOT>
        <EQ>
          <VAR>j1</VAR>
          <PLUS>
            <VAR>j</VAR>
            <INT>1</INT>
          </PLUS>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>k1</VAR>
          <MINUS>
            <VAR>k</VAR>
            <INT>1</INT>
          </MINUS>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>j</VAR>
      <VAR>k</VAR>
      <VAR>n</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <GE>
          <VAR>j</VAR>
          <VAR>n</VAR>
        </GE>
      </NOT>
      <NOT>
        <LE>
          <VAR>k</VAR>
          <INT>-1</INT>
        </LE>
      </NOT>
    </OR>
  </ASSERT>
</VC>

