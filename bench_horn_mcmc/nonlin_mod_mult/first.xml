<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>4</ARITY>
      <NAME>inv</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>i</VAR>
    <VAR>i1</VAR>
    <VAR>j</VAR>
    <VAR>j1</VAR>
    <VAR>k</VAR>
    <VAR>l</VAR>
    <VAR>l1</VAR>
    <VAR>m</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>i</VAR>
      <VAR>j</VAR>
      <VAR>k</VAR>
      <VAR>l</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>i</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
      <NOT>
        <GT>
          <VAR>j</VAR>
          <INT>0</INT>
        </GT>
      </NOT>
      <NOT>
        <GT>
          <VAR>k</VAR>
          <INT>0</INT>
        </GT>
      </NOT>
      <NOT>
        <EQ>
          <VAR>l</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>i</VAR>
      <VAR>j</VAR>
      <VAR>k</VAR>
      <VAR>l</VAR>
    </BEFORE>
    <AFTER>
      <VAR>i1</VAR>
      <VAR>j1</VAR>
      <VAR>k</VAR>
      <VAR>l1</VAR>
    </AFTER>
    <OR>
      <NOT>
        <EQ>
          <VAR>i1</VAR>
          <PLUS>
            <VAR>i</VAR>
            <INT>1</INT>
          </PLUS>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>m</VAR>
          <MOD>
            <VAR>j</VAR>
            <VAR>k</VAR>
          </MOD>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>j1</VAR>
          <PLUS>
            <VAR>j</VAR>
            <VAR>m</VAR>
          </PLUS>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>l1</VAR>
          <PLUS>
            <VAR>l</VAR>
            <VAR>m</VAR>
          </PLUS>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>i</VAR>
      <VAR>j</VAR>
      <VAR>k</VAR>
      <VAR>l</VAR>
    </VARDECLS>
    <OR>
      <LE>
        <VAR>l</VAR>
        <MUL>
          <VAR>k</VAR>
          <VAR>i</VAR>
        </MUL>
      </LE>
    </OR>
  </ASSERT>
</VC>

