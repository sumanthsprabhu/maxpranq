<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>4</ARITY>
      <NAME>inv</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>z0</VAR>
    <VAR>z1</VAR>
    <VAR>x0</VAR>
    <VAR>x1</VAR>
    <VAR>y0</VAR>
    <VAR>y1</VAR>
    <VAR>w0</VAR>
    <VAR>w1</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>x1</VAR>
      <VAR>y1</VAR>
      <VAR>z1</VAR>
      <VAR>w1</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>z1</VAR>
          <VAR>x1</VAR>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>y1</VAR>
          <VAR>w1</VAR>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>x0</VAR>
      <VAR>y0</VAR>
      <VAR>z0</VAR>
      <VAR>w0</VAR>
    </BEFORE>
    <AFTER>
      <VAR>x1</VAR>
      <VAR>y1</VAR>
      <VAR>z0</VAR>
      <VAR>w0</VAR>
    </AFTER>
    <OR>
      <EQ>
        <VAR>x0</VAR>
        <INT>0</INT>
      </EQ>
      <NOT>
        <EQ>
          <VAR>x1</VAR>
          <MINUS>
            <VAR>x0</VAR>
            <INT>1</INT>
          </MINUS>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>y1</VAR>
          <MINUS>
            <VAR>y0</VAR>
            <INT>1</INT>
          </MINUS>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>x1</VAR>
      <VAR>y1</VAR>
      <VAR>z1</VAR>
      <VAR>w1</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>x1</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>z1</VAR>
          <VAR>w1</VAR>
        </EQ>
      </NOT>
      <EQ>
        <VAR>y1</VAR>
        <INT>0</INT>
      </EQ>
    </OR>
  </ASSERT>
</VC>

