<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>3</ARITY>
      <NAME>inv</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>i1</VAR>
    <VAR>i2</VAR>
    <VAR>j1</VAR>
    <VAR>j2</VAR>
    <VAR>k1</VAR>
    <VAR>k2</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>i1</VAR>
      <VAR>j1</VAR>
      <VAR>k1</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>i1</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>j1</VAR>
          <INT>10</INT>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>k1</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>i1</VAR>
      <VAR>j1</VAR>
      <VAR>k1</VAR>
    </BEFORE>
    <AFTER>
      <VAR>i2</VAR>
      <VAR>j2</VAR>
      <VAR>k2</VAR>
    </AFTER>
    <OR>
      <NOT>
        <EQ>
          <VAR>i2</VAR>
          <PLUS>
            <VAR>i1</VAR>
            <INT>1</INT>
          </PLUS>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>j2</VAR>
          <MINUS>
            <VAR>j1</VAR>
            <INT>3</INT>
          </MINUS>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>k2</VAR>
          <PLUS>
            <VAR>k1</VAR>
            <INT>2</INT>
          </PLUS>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>i1</VAR>
      <VAR>j1</VAR>
      <VAR>k1</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <PLUS>
            <VAR>i1</VAR>
            <VAR>k1</VAR>
          </PLUS>
          <INT>6</INT>
        </EQ>
      </NOT>
      <EQ>
        <VAR>j1</VAR>
        <INT>4</INT>
      </EQ>
    </OR>
  </ASSERT>
</VC>

