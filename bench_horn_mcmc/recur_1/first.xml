<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>2</ARITY>
      <NAME>itp1</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>cnt_all_1</VAR>
    <VAR>cnt_all_2</VAR>
    <VAR>st_all_1</VAR>
    <VAR>st_all_2</VAR>
    <VAR>segm1</VAR>
    <VAR>segm2</VAR>
    <VAR>m</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>cnt_all_1</VAR>
      <VAR>st_all_1</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>cnt_all_1</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>st_all_1</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>cnt_all_1</VAR>
      <VAR>st_all_1</VAR>
    </BEFORE>
    <AFTER>
      <VAR>cnt_all_2</VAR>
      <VAR>st_all_2</VAR>
    </AFTER>
    <OR>
      <AND>
        <OR>
          <NOT>
            <EQ>
              <VAR>m</VAR>
              <INT>1</INT>
            </EQ>
          </NOT>
          <NOT>
            <EQ>
              <VAR>cnt_all_2</VAR>
              <PLUS>
                <INT>1</INT>
                <VAR>cnt_all_1</VAR>
              </PLUS>
            </EQ>
          </NOT>
        </OR>
        <OR>
          <EQ>
            <VAR>m</VAR>
            <INT>1</INT>
          </EQ>
          <NOT>
            <EQ>
              <VAR>cnt_all_2</VAR>
              <VAR>cnt_all_1</VAR>
            </EQ>
          </NOT>
        </OR>
      </AND>
      <NOT>
        <EQ>
          <VAR>st_all_2</VAR>
          <PLUS>
            <VAR>st_all_1</VAR>
            <INT>1</INT>
          </PLUS>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>cnt_all_1</VAR>
      <VAR>st_all_1</VAR>
    </VARDECLS>
    <OR>
      <LE>
        <VAR>cnt_all_1</VAR>
        <VAR>st_all_1</VAR>
      </LE>
    </OR>
  </ASSERT>
</VC>

